<div class="my-card">
    <div class="card-header">
        <img src="/static/img/big-waterfall-dario-428.jpg" alt="water_fall" />
    </div>
    <div class="card-body">
        <span class="tag tag-teal">Keyword: {{keyWord}}</span>
        <h4>
            Title: {{title}}
        </h4>
        <div class="user">
            <img src="{{imageUrl}}" alt="vote" />
            <div class="user-info">
                <h5>Author: {{owner.firstName}} {{owner.lastName}}</h5>
                <small>Rating of votes: {{rating}}</small>
            </div>
        </div>
    </div>
</div>