        <main>
            <h1>Edit Cube</h1>
            <div class="form">
                <form action="#" method="POST">
                    <label for="name">Name</label>
                    <input type="text" id="name" name="name" value="">
                    <label for="description">Description</label>
                    <textarea id="description" name="description"></textarea>
                    <label for="image">ImageUrl</label>
                    <input type="text" id="image" name="imageUrl" value="">
                    <label for="difficulty">Difficulty</label>
                    <select id="difficulty" name="difficultyLevel" />
                    <option {{optionSelected}} value="1">1 - Very Easy</option>
                    <option {{optionSelected}} value="2">2 - Easy</option>
                    <option {{optionSelected}} value="3">3 - Medium (Standard 3x3)</option>
                    <option {{optionSelected}} value="4">4 - Intermediate</option>
                    <option {{optionSelected}} value="5">5 - Expert</option>
                    <option {{optionSelected}} value="6">6 - Hardcore</option>
                    </select>
                    <input type="submit" value="Edit">
                </form>
            </div>
        </main>